  <span class="str">"openAs"</span><span class="pun">: </span><span class="kwd">enum (<code translate="no" dir="ltr"><a href="/chat/api/reference/rest/v1/cards#OpenAs">OpenAs</a></code>)</span><span class="pun">,</span>
  <span class="str">"onClose"</span><span class="pun">: </span><span class="kwd">enum (<code translate="no" dir="ltr"><a href="/chat/api/reference/rest/v1/cards#OnClose">OnClose</a></code>)</span>
